{
  "name": "vuex-modules",
  "version": "0.12.0",
  "description": "Exemplo de uso de modulos no Vuex",
  "main": "index.js",
  "config": {
    "commitMessage": "Arquivos HTML migrados para PUG"
  },
  "scripts": {
    "dev": "parcel -p 8080 --no-cache ./src/index.pug",
    "build": "parcel build ./src/index.pug --public-url ./ --no-cache --no-source-maps",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpb",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpb": "git push bb master --tags"
  },
  "keywords": [
    "modules",
    "vuex",
    "example",
    "parcel"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.4",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.2",
    "cross-var-no-babel": "^1.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "pug": "^2.0.4",
    "sass": "^1.23.7",
    "standard": "^14.3.1",
    "vue-template-compiler": "^2.6.10"
  }
}
